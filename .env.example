# --- Authentication ---
# Generate a random secret: openssl rand -base64 32
AUTH_SECRET=****

# --- Database ---
# PostgreSQL Connection String
POSTGRES_URL=postgresql://user:password@host:port/database
# Redis Connection String (Required for resumable streams)
REDIS_URL=redis://host:port

# --- AI Providers ---
# Google Gemini API Key
GEMINI_API_KEY=****

# Vercel AI Gateway (Optional if using direct providers)
AI_GATEWAY_API_KEY=****

# --- File Storage ---
# Vercel Blob Token
BLOB_READ_WRITE_TOKEN=****

# --- Analytics & Feature Flags ---
# Statsig Client Key (Public)
NEXT_PUBLIC_STATSIG_CLIENT_KEY=client-****

# --- Vercel API ---
# Vercel Project ID (e.g. prj_*************)
VERCEL_PROJECT_ID=prj_****
# Vercel Personal Access Token for API requests
VERCEL_TOKEN=****
# Vercel Deploy Hook URL for production builds
VERCEL_DEPLOY_HOOK_URL=https://vercel.com/api/deploy/****
